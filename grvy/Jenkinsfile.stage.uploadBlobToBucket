#!/usr/bin/env groovy

def call(){
    def apiPreparation = load "${env.WORKSPACE}/gcp-poc/grvy/Jenkinsfile.stage.apiPreparation"
    apiPreparation(null)[0]
    def blob_file_path = load "${env.WORKSPACE}/gcp-poc/grvy/Jenkinsfile.stage.fleChkNgcpCompUpdt"
    sh(script: "gsutil cp ${blob_file_path()} gs://${PREPROD_BUCKET}/dialogflowcx/preprod/${params.version}/${params.agent_name}/")
    }

return this